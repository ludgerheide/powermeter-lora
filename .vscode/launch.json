{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "${defaultBuildTask}",
            "name": "Pico Debug",
            "device": "RP2040",
            "gdbPath": "arm-none-eabi-gdb",
            "cwd": "${workspaceRoot}",
            "executable": "./target/thumbv6m-none-eabi/debug/${workspaceFolderBasename}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                "/interface/cmsis-dap.cfg",
                "${workspaceFolder}/rp2040.cfg"
            ],
            "searchDir": [
                "${workspaceRoot}",
                "${env:OPENOCD_PATH}/tcl"
            ],
            "openOCDPreConfigLaunchCommands": [
                "adapter speed 5000"
            ],
            "svdPath": "${workspaceFolder}/rp2040.svd",
            "runToEntryPoint": "main",
            "postRestartCommands": [
                "break main",
                "continue"
            ]
        }
    ]
}